{
  "name": "barebone",
  "version": "2.3.0",
  "description": "Just an example",
  "main": "barebone.js",
  "scripts": {
    "start": "npm-run-all --parallel watch-js watch-sass",
    "watch-js": "webpack-dev-server --config ./tasks/webpack.dev.js --mode development",
    "watch-sass": "node-sass -w --include-path scss scss/barebone-plugin.scss ../Origo_localization/origo/plugins/barebone.css",
    "prewatch-sass": "node-sass --include-path scss scss/barebone-plugin.scss ../Origo_localization/origo/plugins/barebone.css",
    "build": "webpack --config ./tasks/webpack.prod.js && npm run build-sass",
    "build-sass": "node-sass --include-path scss scss/barebone-plugin.scss build/css/barebone.css"
  },
  "author": "Origo",
  "license": "MIT",
  "dependencies": {
    "npm-run-all2": "^6.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.30.0",
    "mini-css-extract-plugin": "^2.9.1",
    "node-sass": "^9.0.0",
    "webpack": "^5.94.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  }
}
